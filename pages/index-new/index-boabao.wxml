<scroll-view scroll-x="true" class='baobao-use'>

  <view class='baobao-thing {{currentTab==0 ? "chooseTab" : ""}}' data-current="0" data-cat-id='{{cat_list[5].cat_id}}' bindtap='goToList'>
    <text class='baobao-some-thing '>{{cat_list[5].title}}</text>
  </view>
  <view class='baobao-thing {{currentTab==1 ? "chooseTab" : ""}}' data-current="1" bindtap='goToList' data-cat-id='{{cat_list[6].cat_id}}'>
    <text class='baobao-some-thing'>{{cat_list[6].title}}</text>
  </view>
  <view class='baobao-thing {{currentTab==2 ? "chooseTab" : ""}}' data-current="2" bindtap='goToList' data-cat-id='{{cat_list[7].cat_id}}'>
    <text class='baobao-some-thing'>{{cat_list[7].title}}</text>
  </view>
  <view class='baobao-thing {{currentTab==3 ? "chooseTab" : ""}}' data-current="3" bindtap='goToList' data-cat-id='{{cat_list[8].cat_id}}'>
    <text class='baobao-some-thing'>{{cat_list[8].title}}</text>
  </view>
  <view class='baobao-thing {{currentTab==4 ? "chooseTab" : ""}}' data-current="4" bindtap='goToList' data-cat-id='{{cat_list[9].cat_id}}'>
    <text class='baobao-some-thing'>{{cat_list[9].title}}</text>
  </view>
</scroll-view>
<!--用品  -->
<view class='rush-baobao-box' wx:if="{{tabList==0}}">
  <block wx:if="{{dataList.length>0}}"   wx:for="{{dataList}}" wx:key="{{index}}">
    <view class='rush-baobao-image' bindtap='goDetails' data-categoryid='{{item.categoryId}}' data-productid='{{item.product_id}}'>
      <image class='rush-bobao-child' src='{{item.image}}'></image>
      <view class='rush-baobao-text'>
        <text class='rush-boabao-title'>{{item.name}}</text>
        <text class='rush-boabao-reduce' wx:if="{{item.specialPrice != null}}">直降{{item.original_price-item.price}}元</text>
        <view class='rush-baobao-price'>
          <view class='rush-baobao-curr-father'>
            <text class='rush-baobao-cn'>￥</text>
            <text class='rush-bobao-current'>{{item.price==null?item.original_price:item.price}}</text>
          </view>
          <text class='rush-baobao-yet' wx:if="{{item.specialPrice!=null}}">￥{{item.original_price}}</text>
          <text class='rush-baobao-sell'>已售{{item.count}}件</text>
        </view>
      </view>
    </view>
  </block>
    <view wx:if="{{dataList.length<1}}" class='no-data'>
      <image src='../../image/no-data.png'></image>
      <view class='no-data-txt'>暂无商品信息</view>
    </view>
</view>
