page{background:#ececec}
.baobao-use{height:80rpx;white-space:nowrap;background:#fff;position:fixed;top:0rpx;z-index:99999999}
.baobao-thing{display:inline-block;font-size:0rpx;width:150rpx;height:80rpx;line-height:80rpx;text-align:center;color:#999}
