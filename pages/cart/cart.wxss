.cart-total {
  width: 450rpx;
  display: block;
}

.cart-tot-title {
  font-size: 28rpx;
  margin-left: 10rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  height: 76rpx;
}

.cart-size-list {
  height: 40rpx;
  line-height: 40rpx;
  display: flex;
  justify-content: space-between;
}

.cart-text-list {
  font-size: 24rpx;
  margin-left: 10rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #999;
}

.cart-img-list {
  width: 30rpx;
  height: 30rpx;
  margin-left: 20rpx;
}

.cart-frice-list {
  width: 420rpx;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-left: 5rpx;
  height: 50rpx;
  line-height: 50rpx;
  color: #FE1C1C;
  
}

.cart-common {
  width: 50rpx;
  height: 40rpx;
  border: 1px solid #939393;
  text-align: center;
  font-size: 30rpx;
  line-height: 40rpx;
  padding-left: 5rpx;
  padding-right: 5rpx;
}
.cart-common-two {
  min-width: 50rpx;
  height: 40rpx;
  border: 1px solid #939393;
  text-align: center;
  font-size: 30rpx;
  line-height: 40rpx;
  padding-left: 5rpx;
  padding-right: 5rpx;
}
.cart-reduce,.cart-text-num{
  border-right: none;
}
/*价格*/

.carts-price {
  color: #f60;
}

/*底部按钮*/

.carts-footer {
  width: 750rpx;
  height: 100rpx;
  justify-content: space-between;
  position: fixed;
  bottom: 0rpx;
  border-top: 1rpx solid #eee;
  background: #E4E4E4;
  line-height: 100rpx;
  z-index: 99999999;
}

/*复选框*/

.carts-footer icon {
  margin-left: 20rpx;
  color: #fff;
  display: block;
  margin-top: 25rpx;
}

/*全选字样*/

.carts-footer .total-go {
  font-size: 30rpx;
}

/*立即结算按钮*/

.carts-footer .button {
  line-height: 100rpx;
  text-align: center;
  width: 220rpx;
  background-color: #FFe546;
  font-size: 34rpx;
  font-weight: 700;
  border-radius: 0;
  border: 0;
  padding-left: 55rpx;
}

.stepper {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding-top: 40rpx;
}

.stepper text {
  background-color: #f2f2f2;
  margin: 5px 0;
  width: 26px;
  height: 26px;
  line-height: 26px;
  text-align: center;
  font-weight: 900;
  color: #939393;
  border-radius: 6rpx;
  border: 1px solid #fff;
  /*border-left:1px solid #fff ;*/
}

.stepper input {
  width: 43px;
  height: 25px;
  text-align: center;
  /*background-color: #f2f2f2;*/
  border-radius: 6rpx;
  border-top: 1px solid #f2f2f2;
  border-bottom: 1px solid #f2f2f2;
}

.modal-close {
  margin-left: 200rpx;
  padding: 20px 0;
  color: #939393;
}

.page-list {
  /* padding-bottom: 60rpx; */
  background: #ffffff;
}

