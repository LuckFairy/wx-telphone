/* pages/saoma/coupon-list.wxss */
/*优惠券区域  */
.goods-detail-containner{
  background: #eee;
}
.discount-area{
  margin: 16rpx 0;
  flex-direction: column;
}
.discount {
  background: #fff;
  display: flex;
  width: 750rpx;
  height: 88rpx;
  align-items: center;
  justify-content: space-between;
  padding: 0 24rpx;
}
.discount-imglist{
  flex: 1;
}
.discount-img1:first-child{
  margin-left: 34rpx;
}
.discount-img1{
  display: inline-block;
  color: #ff3030;
  font-size: 28rpx;
  width: 134rpx;
  height: 46rpx;
  padding:0 26rpx 0 28rpx;
  line-height: 46rpx;
  background-position: center;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-image: url("data:image/png;base64,");
}

.discount-go{
  display: inline-block;
  vertical-align: -7%;
  height: 17rpx;
  width: 29rpx;
}
.discount-text {
  font-size: 28rpx;
  color: #1b1b1b;
}

.discount-ticket {
  font-size: 24rpx;
  color: #1b1b1b;
  margin-right: 20rpx;
  display: inline-block;
}


.discount-ticket-price {
  font-size: 24rpx;
  color: #1b1b1b;
  display: inline-block;
}

.discount-list {
  box-sizing: border-box;
  height: 140rpx;
  border-top: 1rpx solid #ececec;
  padding:0 24rpx;
  justify-content: space-between;
  align-content: center;
  background: #fff;
}
.discount-left{
  justify-content: flex-start;

}
.discount-img2{
  width: 134rpx;
  height: 80rpx;
  margin:30rpx 0;
 margin-right: 24rpx;
  color: #fff;
  align-content: center;
  justify-content: center;
  line-height: 80rpx;
  font-size: 36rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-image: url("data:image/png;base64,");
}
.discount-list-text{
  flex-direction: column;
}
.discount-name{
  color:inherit;
  font-size: 28rpx;
  height: 28rpx;
  margin-top: 30rpx;
}
.discount-time{
  color:#999;
  font-size: 24rpx;
  margin-top: 16rpx;
  margin-bottom: 38rpx;
}
.discount-color-red {
  font-size: 30rpx;
  color: #ff3030;
}
.discount-btn,.discount-btn-no{
  box-sizing: border-box;
  font-size: 26rpx;
  color:#ff3030;
  text-align: center;
  border: 2rpx solid #ff3030;
  border-radius: 10rpx;
  width: 130rpx;
  height: 46rpx;
  line-height: 46rpx;
  margin:47rpx 0;
  margin-left: 20rpx;
}
.discount-btn-no{
  color:#999;
  border-color: #999;
}
.no-goods{
  background-color: #ccc;
}