/* pages/card/mycard.wxss */

page {
  width: 100%;
  height: 100%;
  background: #efefef;
}

view {
  display: flex;
}

swiper {
  display: flex;
  height: 100%;
  width: 750rpx;
  /* padding-top: 80rpx; */
}

scroll-view {
  height: 100%;
  display: block;
  background-color: #ecf2f9;
  _padding-top: 10px;
}

text {
  display: block;
  text-align: center;
  overflow: hidden;
  margin: 0px;
  padding: 0px;
}

.main-container {
  flex-direction: column;
  width: 750rpx;
  height: 100%;
}

.nav-header-view {
  flex-direction: row;
  justify-content: space-around;
  align-items: flex-end;
  width: 750rpx;
  height: 80rpx;
  /* margin-bottom: -80rpx; */
  font-size: 25rpx;
  color: grey;
  _box-shadow: 0px 5px 10px rgba(1, 1, 1, 0.2);
  z-index: 2;
  background: #fff;
}

.header-col-view {
  flex: 1;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border-bottom-style: solid;
  border-bottom-width: 2px;
  border-bottom-color: rgba(1, 1, 1, 0);
  height: 80rpx;
  padding-top: 5px;
  color: #999999;
}

.show-border-bottom {
  border-bottom-color: #ffcc3c;
  color: #1b1b1b;
}

/*  -------------优惠券-------------------- */
.keyong-card{
  font-size: 30rpx;
}
.card-list-view {
  display: flex;
  /* justify-content: space-between; */
  flex-wrap: wrap;
  width: 750rpx;
  padding-bottom: 15px;
  /* margin-top:10px; */
  margin-bottom: 206rpx;
  align-items: center;
  font-size: 0rpx;
  margin-left: 20rpx;
}

.card-ad-view {
  display: flex;
  flex-direction: row;
  width: 750rpx;
  justify-content: center;
  margin-bottom: 10px;
}

.card-ad-img {
  width: 750rpx;
}

.card-item-view {
  flex-direction: row;
  justify-content: space-between;
  width: 710rpx;
  height: 105px;
  margin-bottom: 10px;
}

.card-item-left {
  flex-direction: column;
  justify-content: space-around;
  padding-left: 15rpx;
  padding-bottom: 0px;
}

.card-item-title-view {
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}

.card-item-mch-icon {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  margin-right: 10px;
}

.card-title-text {
  font-size: 35rpx;
  font-weight: bold;
  white-space: nowrap;
  overflow: hidden;
}

.card-info-text-normal {
  color: grey;
  font-size: 26rpx;
  margin-bottom: 10rpx;
}

.card-info-text-small {
  color: grey;
  font-size: 23rpx;
  height: 34rpx;
}

.card-item-right {
  flex-direction: column;
  justify-content: space-between;
  padding: 10px;
}

.card-bg-available {
}

.card-bg-disable {
}

.card-item-view {
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  width: 710rpx;
  height: 180rpx;
  background-size: 710rpx 180rpx;
} 

.card-item-value {
  flex-direction: row;
  justify-content: flex-end;
  align-items: flex-end;
  width: 100%;
  color: white;
  font-size: 30rpx;
}

.card-item-btn-view {
  flex-direction: row;
  justify-content: flex-end;
  align-items: flex-end;
  width: 100%;
  color: white;
  font-size: 20rpx;
}

.overlay {
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 0px;
  left: 0px;
  width: 750rpx;
  height: 100%;
  background-color: rgba(10, 10, 10, 0.3);
}

.qr-view {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 500rpx;
  background-color: white;
  color: grey;
  font-size: 25rpx;
  padding-bottom: 20px;
  position: relative;
}

.qr-image {
  width: 400rpx;
}

.close-icon {
  position: absolute;
  right: 2px;
  top: 0px;
  font-size: 35rpx;
}

.no-data-view {
  flex-direction: column;
  align-items: center;
  padding-top: 150rpx;
}

.no-data-image {
  width: 200rpx;
  margin-bottom: 40rpx;
}

.no-data-text {
  text-align: center;
  color: #666;
  line-height: 130%;
}

.go-add {
  font-size: 22rpx;
}

.use-ruler {
  width: 750rpx;
  height: 80rpx;
  background: #efefef;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  padding-right: 20rpx;
}

.use-ruler-image {
  width: 30rpx;
  height: 30rpx;
  padding-right: 10rpx;
}

.use-ruler-text {
  font-size: 26rpx;
  color: #1b1b1b;
}

.cards-use-list {
  width: 660rpx;
  height: 180rpx;
  background-size: 660rpx 180rpx;
  background-repeat: no-repeat;
}
.dedicated{
  background-image: url("data:image/png;base64,");
}
.common{
  background-image: url("data:image/png;base64,");
}
.cards-use-list-two {
  width: 660rpx;
  height: 180rpx;
  background-image: url("data:image/png;base64,");
  background-size: 660rpx 180rpx;
  background-repeat: no-repeat;
}

.icon-ruler {
  display: inline-block;
  margin-right: 5rpx;
}

.rulers-text-title {
  font-size: 30rpx;
  color: #1b1b1b;
  text-align: left;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  width: 430rpx;
}

.rulers-text-date {
  font-size: 24rpx;
  color: #999;
  text-align: left;
  margin-top: 10rpx;
}

.rulers-text-id {
  font-size: 24rpx;
  color: #1b1b1b;
  text-align: left;
  margin-top: 19rpx;
}

.rulers-text-list {
  display: block;
  width: 480rpx;
  margin-left: 24rpx;
  margin-top: 24rpx;
}

.rulers-prices {
  font-size: 50rpx;
  color: #fff;
  height: 160rpx;
  line-height: 160rpx;
  width: 186rpx;
}

.rules-wraps {
  display: flex;
  align-items: center;
  margin-bottom: 20rpx;
}

.normal-block {
  display: flex;
  align-items: center;
}

.bottom-fixed {
  position: fixed;
  bottom: 0rpx;
  height: 96rpx;
  width: 750rpx;
  background: #fff;
  z-index: 99999999;
}

.bottom-fixed-left {
  width: 450rpx;
  line-height: 96rpx;
  font-size: 28rpx;
  text-align: left;
  justify-content: flex-end;
  margin-right: 28rpx;
}

.bottom-fixed-go {
  width: 300rpx;
  background: #ffe71c;
  line-height: 96rpx;
  font-size: 30rpx;
  color: #1b1b1b;
}
