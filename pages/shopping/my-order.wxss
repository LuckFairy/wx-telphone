/* pages/shopping/my-order.wxss */

page {
  width: 100%;
}

view {
  display: flex;
}

swiper {
  display: flex;
  height: 100%;
  width: 750rpx;
}

scroll-view {
  height: 100%;
  display: block;
  background-color: #f3f3f3;
  _padding-top: 10px;
}

text {
  display: block;
  overflow: hidden;
  font-size: 28rpx;
  background-color: FFE546;
  margin: 0px;
  padding: 0px;
}

.main-container {
  width: 750rpx;
  display: block;
  height: 100%;
}

.nav-header-view {
  width: 750rpx;
  display: -webkit-box;
  white-space: nowrap;
  overflow: hidden;
   -webkit-backface-visibility: hidden;
  -webkit-perspective: 1000; 
  -webkit-overflow-scrolling: touch;
  text-align: justify;
  overflow-x: initial;
  position: fixed;
  top: 0rpx;
  z-index: 99999999999;
  opacity: 1;
  background: #ffffff;
}
.scroll-lists{
  margin-top: 80rpx;
}
.header-col-view {
  height: 80rpx;
  width: 150rpx;
  line-height: 80rpx;
  text-align: center;
  display: inline-block;
}

.show-border-bottom {
  border-bottom: 4rpx solid #ffcc3c;
}

/*  -------------订单列表-------------------- */

.order-list-view {
  flex-direction: column;
  align-items: center;
  width: 750rpx;
  padding-bottom: 80rpx;
  margin-top: 10px;
}
.order-item-view {
  flex-direction: column;
  align-items: center;
  width: 750rpx;
  background-color: white;
  margin-bottom: 10px;
}

.goods-info-view {
  flex-direction: row;
  justify-content: space-between;
  width: 700rpx;
  margin-top: 10px;
}

.goods-img {
  width: 160rpx;
  height: 160rpx;
}

.goods-name-view {
  font-size: 25rpx;
  flex: 1;
  margin-left: 10rpx;
  margin-top: 10rpx;
  flex-direction: row;
}

.goods-name {
  flex: 1;
  font-size: 30rpx;
}

.goods-price-view {
  width: 700rpx;
  flex-direction: row;
  justify-content: flex-end;
  font-size: 25rpx;
  border-top: 1px solid #f4f4f4;
  border-bottom: 1px solid #f4f4f4;
  padding: 8px 0px;
}

.yellow-btn-view {
  width: 700rpx;
  flex-direction: row;
  justify-content: flex-end;
  margin-top: 10px;
  margin-bottom: 10px;
}

.yellow-btn {
  background-color: #fae53b;
  color: #605820;
  padding: 15rpx 20px;
  font-size: 28rpx;
  border-radius: 5rpx;
  line-height: 100%;
  border: 2rpx solid #fae53b;
}

.yellow-one, .yellow-two {
  margin-right: 20rpx;
  background-color: #fff;
  color: #605820;
  padding: 15rpx 20px;
  font-size: 28rpx;
  border-radius: 5rpx;
  line-height: 100%;
  border: 2rpx solid #efefef;
}

.yellow-two {
  background-color: #ffe546 !important;
}

.no-order-view {
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  padding-top: 40px;
  font-size: 25rpx;
}

.gobuy-btn-view {
  background-color: #ffe547;
  color: #8e7c22;
  padding: 10px 100rpx;
  margin-top: 30px;
}

.msg-containner {
  flex-direction: column;
  align-items: center;
  background-color: white;
  border-radius: 5px;
}

.confirm-text-view {
  padding: 20px 50rpx;
  flex-direction: column;
  align-items: center;
  font-size: 30rpx;
}

.del-btn-view {
  background-color: #d9d9d9;
  width: 100%;
  flex-direction: row;
  justify-content: center;
  border-radius: 0 0 5px 5px;
}

.del-btn-text {
  padding: 10px 30rpx;
  flex: 1;
  font-size: 30rpx;
}

.overlay {
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 0px;
  left: 0px;
  width: 750rpx;
  height: 100%;
  background-color: rgba(10, 10, 10, 0.3);
}

.qr-view {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 500rpx;
  background-color: white;
  color: grey;
  font-size: 25rpx;
  padding-bottom: 20px;
  position: relative;
}

.close-icon {
  position: absolute;
  right: 2px;
  top: 0px;
  font-size: 35rpx;
}

.no-data-view {
  flex-direction: column;
  align-items: center;
  padding-top: 150rpx;
}

.no-data-image {
  width: 200rpx;
  margin-bottom: 40rpx;
}

.no-data-text {
  text-align: center;
  color: #666;
  line-height: 130%;
}

.no-data-button {
  margin-top: 60rpx;
  padding: 20rpx 0;
  text-align: center;
  width: 426rpx;
}

