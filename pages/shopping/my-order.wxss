}

.invite-friend {
  margin-right: 32rpx;
  padding-left: 0rpx;
  padding-right: 0rpx;
  border-radius: 10rpx;
  background: #fff;
  box-sizing: content-box;
  text-decoration: none;
  border:1rpx solid #ff3131 !important;
  color: #ff3131 !important;
}

button::after {
  content: "";
  width: 200%;
  height: 200%;
  position: absolute;
  top: 0;
  left: 0;
  border: 1px solid rgba(0, 0, 0, 0);
  -webkit-transform: scale(0);
  transform: scale(0);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  box-sizing: border-box;
  border-radius: 10px;
}

.invite-friend-detail {
  margin: 10rpx 24rpx 10rpx 270rpx;
}

.all-list:not(:last-child) {
  margin-bottom: 16rpx;
}

