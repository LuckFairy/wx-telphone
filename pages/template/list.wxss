.section-list,.section-list-row{display:-webkit-box;display:-webkit-flex;-webkit-box-direction:normal}.section-list,.section-list-row,.section-list-row-info{-webkit-box-direction:normal}.section-list{background-color:#fff;display:flex;-webkit-box-orient:vertical;-webkit-flex-direction:column;flex-direction:column;width:100%}.section-list-row{display:flex;-webkit-box-orient:horizontal;-webkit-flex-direction:row;flex-direction:row;padding:15rpx 30rpx;border-bottom:1px solid #eee}.section-list-row-image{height:120rpx;width:120rpx;margin-right:30rpx}.section-list-row-info{-webkit-box-flex:1;-webkit-flex:1;flex:1;height:120rpx;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-orient:vertical;-webkit-flex-direction:column;flex-direction:column;padding-top:5rpx;padding-bottom:5rpx}.section-list-row-title{margin-bottom:10rpx;height:40rpx;font-size:14px;color:#000}.section-list-row-summary{height:60rpx;line-height:30rpx;font-size:12px;color:#888;overflow:hidden;text-overflow:ellipsis}