/* pages/card/mycard.wxss */

page {
  width: 100%;
  height: 100%;
  background: #efefef;
}

swiper {
  display: flex;
  height: 100%;
  width: 750rpx;
}

scroll-view {
  height: 100%;
  display: block;
  /* background-color: #ecf2f9; */
  background-color: #eee;
  _padding-top: 10px;
}

text {
  display: block;
  text-align: center;
  overflow: hidden;
  margin: 0px;
  padding: 0px;
}

.main-container {
  flex-direction: column;
  width: 750rpx;
  margin-top: 104rpx;
}

/*搜索区域  */

.search-msg {
  display: flex;
  width: 750rpx;
  height: 88rpx;
  border-top: 1rpx solid #efefef;
  background: #fff;
  margin-bottom: 16rpx;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  z-index: 9999;
}

.search-input {
  width: 530rpx;
  height: 60rpx;
  position: relative;
  background: #efefef;
  margin-left: 21rpx;
  margin-top: 14rpx;
  border-radius: 5rpx;
}

.search-image {
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  left: 20rpx;
  top: 15rpx;
}

.search-text {
  position: absolute;
  font-size: 26rpx;
  color: #1b1b1b;
  left: 60rpx;
  display: inline-block;
  width: 450rpx;
  height: 60rpx;
}

.search-select {
  position: relative;
  height: 80rpx;
  display: block;
  line-height: 80rpx;
  margin-left: 34rpx;
  margin-top: 7rpx;
}

.search-current {
  font-size: 0rpx;
  height: 80rpx;
  line-height: 80rpx;
}

.search-current-text {
  font-size: 28rpx;
  color: #1b1b1b;
  height: 80rpx;
  display: inline-block;
}

.search-current-trangle {
  display: inline-block;
  width: 0;
  height: 0;
  border-left: 10rpx solid transparent;
  border-right: 10rpx solid transparent;
  border-top: 15rpx solid black;
  margin-top: 32rpx;
  margin-left: 18rpx;
}

.wait-choose {
  position:fixed;
top:92rpx;
z-index:999999999;
background:#ffffff;
width:750rpx;
font-size:0rpx;
display:block;
height:108rpx;
line-height:108rpx;
padding-top:23rpx;

}

.tranClass {
  height: 112rpx;
  transition: height 2s;
  -webkit-transition: width 2s;
}

.online-offline {
  display:inline-block;
font-size:28rpx;
height:60rpx;
line-height:60rpx;
width:160rpx;
border:1rpx solid #efefef;
border-radius:50rpx;
margin-left:24rpx;

}

/*搜索区域结束  */

/*  */

/*列表数据  */

.mom-list {
  width: 750rpx;
  height: 200rpx;
  display: flex;
  justify-content: space-between;
  background-size: 710rpx 200rpx;
  background-repeat: no-repeat;
  background-position: 20rpx 0rpx;
  margin-bottom: 20rpx;
}

.trues {
  background-image: url("data:image/png;base64,");
}

.mom-list-two {
  width: 750rpx;
  height: 200rpx;
  display: flex;
  justify-content: space-between;
  background-image: url("data:image/png;base64,");
  background-size: 710rpx 200rpx;
  background-repeat: no-repeat;
  background-position: 20rpx 0rpx;
  margin-bottom: 20rpx;
}

.mendiancard {
  background-image: url("data:image/png;base64,");
}

.mon-list-left {
  display: block;
}

.mon-image-title {
  display: block;
  font-size: 0rpx;
}

.mon-image {
  width: 70rpx;
  height: 70rpx;
  display: inline-block;
  margin-top: 20rpx;
  margin-left: 36rpx;
}

.mon-title {
  display: inline-block;
  font-size: 32rpx;
  color: #1b1b1b;
  vertical-align: top;
  margin-top: 20rpx;
  text-align: left;
  width: 400rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-left: 50rpx;
}

.mon-price-day {
  display: block;
  font-size: 0rpx;
  margin-top: 18rpx;
}

.mon-price {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  width: 450rpx;
}

.mon-price, .mon-day {
  font-size: 28rpx;
  color: #a9a9a9;
  margin-left: 50rpx;
  display: block;
  text-align: left;
}

.mon-day {
  margin-top: 25rpx;
  overflow: hidden;
  width: 450rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.mon-list-right {
  display: block;
}

.mon-money-right {
  font-size: 0rpx;
  display: block;
  width: 210rpx;
  text-align: center;
  margin-top: 34rpx;
}

/* .money-right-cn {
  display: inline-block;
  font-size: 40rpx;
  color: #fff;
  margin-top: 15rpx;
} */

.money-right-text {
  display: inline-block;
  font-size: 40rpx;
  color: #fff;
}

.mon-right-go {
  display: block;
  margin-top: 30rpx;
  justify-content: center;
  color: #fff;
  font-size: 28rpx;
  border: 1rpx solid #fff;
  border-radius: 6rpx;
  width: 140rpx;
  margin-left: 29rpx;
}
.shopCard{
  background: rgb(255, 231, 28);
}
.xianshangCard{
  background: rgb(255, 231, 28);
}

