/* pages/card/mycard.wxss */

page {
  width: 100%;
  height: 100%;
  background: #efefef;
}


swiper {
  display: flex;
  height: 100%;
  width: 750rpx;
}

scroll-view {
  height: 100%;
  display: block;
  /* background-color: #ecf2f9; */
   background-color: #eee; 
  _padding-top: 10px;
}

text {
  display: block;
  text-align: center;
  overflow: hidden;
  margin: 0px;
  padding: 0px;
}

.main-container {
  flex-direction: column;
  width: 750rpx;
  margin-top: 104rpx;
}


/*搜索区域  */

.search-msg {
  display: flex;
  width: 750rpx;
  height: 88rpx;
  border-top: 1rpx solid #efefef;
  background: #ffffff;
  margin-bottom: 16rpx;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  z-index: 9999;
}

.search-input {
  width: 530rpx;
  height: 60rpx;
  position: relative;
  background: #efefef;
  margin-left: 21rpx;
  margin-top: 14rpx;
  border-radius: 5rpx;
}

.search-image {
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  left: 20rpx;
  top: 15rpx;
}

.search-text {
  position: absolute;
  font-size: 26rpx;
  color: #1b1b1b;
  left: 60rpx;
  display: inline-block;
  width: 450rpx;
  height: 60rpx;
}

.search-select {
  position: relative;
  height: 80rpx;
  display: block;
  line-height: 80rpx;
  margin-left: 34rpx;
  margin-top: 7rpx;
}

.search-current {
  font-size: 0rpx;
  height: 80rpx;
  line-height: 80rpx;
}

.search-current-text {
  font-size: 28rpx;
  color: #1b1b1b;
  height: 80rpx;
  display: inline-block;
}

.search-current-trangle {
  display: inline-block;
  width: 0;
  height: 0;
  border-left: 10rpx solid transparent;
  border-right: 10rpx solid transparent;
  border-top: 15rpx solid black;
  margin-top: 32rpx;
  margin-left: 18rpx;
}
.wait-choose{
  position: absolute;
  top: 15rpx;
  left: -20rpx;
  z-index: 999999999;
  background: #ffffff;
  width: 110rpx;
  font-size: 0rpx;
  display: block;
  height: 112rpx;
}
.tranClass{
  height: 112rpx;
  transition:height 2s;
  -webkit-transition:width 2s; 
}
.online-offline{
  display: block;
  font-size: 28rpx;
  height: 56rpx;
  line-height: 56rpx;
}
/*搜索区域结束  */
