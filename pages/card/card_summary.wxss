.flex-row-center-all, view {
  display: -webkit-box;
  display: -webkit-flex;
}

page {
  height: 100%;
  font-size: 28rpx;
  color: #fff;
}

.card-name-view, .card-title-view {
  font-size: 30rpx;
  font-weight: 700;
}

view {
  display: flex;
}

.hide {
  display: none;
}

.flex-row-center-all {
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.flex-colunm-center-all, .overlay {
  display: -webkit-box;
  display: -webkit-flex;
}

.flex-colunm-center-all {
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.card-summary-containner {
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  background-color: #ffe546;
  height: 100%;
  padding-top: 20px;
}

.card-head-view, .summary-detail-view {
  -webkit-box-direction: normal;
  background-color: #fff;
}

.card-smy-view {
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  width: 710rpx;
}

.card-head-view {
  border-top-left-radius: 5px;
  border-top-right-radius: 5px;
  padding-left: 10px;
  padding-top: 10px;
  -webkit-box-orient: horizontal;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.mch-icon {
  width: 30px;
  height: 30px;
  border-radius: 50%;
  margin-right: 10px;
}

.split-img-view {
  width: 100%;
}

.summary-detail-view {
  -webkit-box-orient: vertical;
  -webkit-flex-direction: column;
  flex-direction: column;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
  justify-content: flex-start;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  border-bottom-left-radius: 5px;
  border-bottom-right-radius: 5px;
}

.card-name-view {
  margin-top: 25px;
}

.card-money-view {
  margin-top: 10px;
  font-size: 55rpx;
  font-weight: 700;
}

.card-category-view {
  margin-top: 10px;
  font-size: 20rpx;
  color: #fff;
}

.card-category-text {
  border-radius: 17rpx;
  background-color: #ff1d33;
  padding: 5rpx 30rpx;
}

.card-vlidate-date-view {
  margin-top: 20px;
  color: grey;
  font-size: 20rpx;
}

.card-btn-save, .card-btn-use {
  padding: 10px 0rpx;
  font-size: 25rpx;
  width: 350rpx;
  color: grey;
}

.card-btn-use {
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  margin-top: 40px;
  background-color: #ffe546;
  border-radius: 5px;
}

.card-btn-save {
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  border: 1px solid grey;
  border-radius: 5px;
  margin-top: 10px;
}

.func-list-introduction-view, .func-list-view {
  padding: 25rpx 20rpx;
  -webkit-box-direction: normal;
  width: 100%;
  border-top: 1px solid #f7f7f7;
}

.func-list-view {
  -webkit-box-orient: horizontal;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.func-list-introduction-view {
  -webkit-box-orient: vertical;
  -webkit-flex-direction: column;
  flex-direction: column;
}

.overlay {
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  width: 750rpx;
  height: 100%;
  background-color: rgba(10, 10, 10, 0.3);
}

.first_pull_down, .qr-view {
  -webkit-flex-direction: column;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
}
/*扫码核销弹窗  */
.qr-view {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  -webkit-box-pack: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  background-color: #fff;
  color: grey;
  font-size: 26rpx;
  position: relative;
  margin-top: -165rpx;
  width: 600rpx;
  height: 900rpx;
  padding-bottom: 40rpx;
  border-radius: 5px;
}
.qr-title{
  height: 88rpx;
  width: 100%;
  background: #fafafa;
  justify-content: space-between;
  align-items: center;
}
.qr-title-left{
  align-items: center;
  font-size: 43rpx;
  color: #29a54c;
}
.qr-title-left image{
  width: 36rpx;
  height: 36rpx;
  margin: auto 20rpx;
}
.qr-title>image{
  width: 36rpx;
  height:36rpx;
  margin-right: 20rpx;
}
.qr-view .code-txt{
  margin-top: 50rpx ;
  color: #313131;
  font-size: 32rpx;
}
.code-imgone{
  margin-top: 50rpx;
  width: 280rpx;
  height: 280rpx;
}
.code-imgtwo{
  margin-top: 50rpx;  
  height: 120rpx;
  width: 520rpx;
}
.qrcode-list {

  color: #999;
  font-size: 26rpx;
  display: block;
}  
/*核销结束  */
.close-icon {
  position: absolute;
  right: 2px;
  top: 0;
  font-size: 55rpx;
}

.first_pull_down {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.8);
  padding: 160rpx 45rpx 0;
  flex-direction: column;
}

.get_code, .pull_down_img {
  top: 20rpx;
  position: absolute;
}

.first_pull_down>view {
  width: 660rpx;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  box-shadow: 0 8rpx 6rpx 0 #fe6422;
  background: -webkit-linear-gradient(left, #f47e16, #ff6123);
  background: linear-gradient(to right, #f47e16, #ff6123);
  margin-bottom: 6rpx;
}

.first_pull_view {
  padding: 35rpx 0rpx 20rpx;
  font-size: 46rpx;
  text-align: center;
}

.pull_down_top {
  display: inline-block;
  width: 660rpx;
  text-align: center;
}

.pull_down_bottom {
  text-align: center;
  margin-bottom: 34rpx;
}

.pull_down_img {
  width: 40rpx;
  height: 40rpx;
  right: 20rpx;
}

.first_pull_form {
  background-color: #fff !important;
  width: 560rpx;
  padding: 0 50rpx;
}

.first_pull_login {
  padding: 36rpx 0rpx;
  border-bottom: 1rpx solid #c7c7c7;
}

.first_pull_login>label {
  color: #999;
  margin-right: 60rpx;
  margin-top: 7rpx;
  width: 108rpx;
}

.pull_label_3 {
  margin-right: 30rpx;
}

.pull_input_3 {
  width: 180rpx;
}

.first_pull_login>input {
  color: #000;
  vertical-align: middle;
}

input-placeholder {
  color: #c7c7c7;
}

.get_code {
  padding: 22rpx 30rpx;
  color: #f67919;
  border: solid #f67919;
  vertical-align: middle;
  right: 0rpx;
}

.errMsg, .post_code {
  border-radius: 10rpx;
}

.input-placeholder {
  color: #c7c7c7;
}

.post_code {
  margin-top: 72rpx;
  background-color: #f47e16;
  font-size: 30rpx;
  padding: 32rpx 0rpx;
  width: 560rpx;
  margin-bottom: 50rpx;
  color: #fff;
}

.errMsg {
  display: block;
  width: 420rpx;
  padding: 37rpx 0;
  background-color: rgba(0, 0, 0, 0.8);
  text-align: center;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-left: -210rpx;
  margin-top: -100rpx;
}

.frist {
  -webkit-animation: myfirst 5s;
  animation: myfirst 5s;
}

@-webkit-keyframes myfirst {
  from {
    left: 5rpx;
  }

  to {
    left: 50rpx;
  }
}

@keyframes myfirst {
  from {
    left: 5rpx;
  }

  to {
    left: 50rpx;
  }
}


