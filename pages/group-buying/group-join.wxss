/*拼团购  */
.group-shop-father{
  display: block;
  width: 750rpx;
}
.group-shop{
  width: 367rpx;
  display: inline-block;
  background: #fff;
  margin-top: 15rpx;
}
.group-shop-image{
  width: 367rpx;
  height: 367rpx;
}
.group-shop-title{
  width: 335rpx;
  font-size: 24rpx;
  display: block;
  margin: 0 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  margin-bottom: 18rpx;
}
.bac-color{
   background: #EFEFEF;
   width: 100%;
}
.group-shop:nth-child(2n){
  margin-left: 16rpx;
}
.group-shopping-wraps{
  display: flex;
  justify-content: space-between;
  margin: 0rpx 24rpx;
}
.group-shop-num{
  font-size: 24rpx;
  display: inline-block;
  text-align: left;
 color: #FF0000
}
.group-shop-num2{
  font-size: 34rpx;
  color: #FF0000
}
.group-shop-num-two{
  color: #999999;
}
.group-shop-bottom{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 65rpx;
  margin-bottom: 20rpx;
  padding: 0 24rpx;
  width: 367rpx;
}
.group-shopping-father{
  font-size: 0rpx;
  display: inline-block;
}
.shopping-go-group{
  width: 130rpx;
  font-size: 24rpx;
  background: #FF0000;
  display: inline-block;
  height: 50rpx;
  line-height: 50rpx;
  color: #ffffff;
  border-radius: 5rpx;
  text-align: center;
}
.gro-pre{
  font-size: 24rpx;
  display: block;
}
.shopping-go-group-end{
  width: 130rpx;
  font-size: 24rpx;
  background: #999999;
  display: inline-block;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  color: #ffffff;
  border-radius: 5rpx;
}



