/*尺码、颜色  */

.commodity_attr_list {
  background: #fff;
  font-size: 26rpx;
  overflow: hidden;
  width: 100%;
  display: block;
}

.attr_box {
  width: 100%;
  overflow: hidden;
  border-bottom: 1rpx solid #ececec;
}

.attr_name {
  width: 20%;
  float: left;
  padding: 15rpx 0;
  font-size: 30rpx;
  margin-top: 20rpx;
}

.attr_value_box {
  width: 80%;
  float: left;
  padding: 15rpx 0;
  overflow: hidden;
}

.attr_value {
  padding: 10rpx 20rpx;
  float: left;
  margin: 0 10rpx;
  border: 1rpx solid #ececec;
  font-size: 33rpx;
}

.attr_value_disabled {
  color: #ccc;
}

.attr_value_active {
  background: #dd2727;
  border-radius: 10rpx;
  color: #fff;
  padding: 10rpx 20rpx;
}

.stepper-wrap {
  background: #fff;
}

/*数量开始  */

.stepper {
  height: 50rpx;
  overflow: hidden;
  margin-left: 24rpx;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
  width: 726rpx;
  display: inline-block;
}

.ste-reduce, .ste-input-num, .ste-add {
  display: inline-block;
  font-size: 30rpx;
}

.ste-reduce, .ste-add {
  width: 56rpx;
  color: #1b1b1b;
  line-height: 50rpx;
  text-align: center;
  background: #f5f5f5;
}

.ste-reduce {
  border-right: 1rpx solid #fff;
}

.ste-add {
  border-left: 1rpx solid #fff;
}

.ste-input-num {
  width: 65rpx;
  line-height: 50rpx;
  color: #1b1b1b;
  text-align: center;
  height: 50rpx;
  background: #e5e5e5;
}

.ste-number {
  font-size: 30rpx;
  height: 50rpx;
  line-height: 50rpx;
}

.stepper-father {
  font-size: 0rpx;
  float: right;
  margin-right: 24rpx;
}

/*数量结束  */

