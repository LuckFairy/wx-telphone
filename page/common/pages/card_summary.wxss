page{
  background: #ffa438;
  width: 100%;
  height: auto;
}
.box{
  margin: 35rpx;
  width: 680rpx;
  /* height: 812rpx; */
  border-radius: 8rpx;
  box-shadow:  10rpx 16rpx 0.6 #ff8400;
  margin-top: 80rpx;
  flex-direction: column;
}
.qr-top{
  height: 96rpx;
background: #fff;
font-size:28rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(66,66,66,1);
  align-items: center;
}
.qr-top image{
  margin: 0 20rpx 0 30rpx;
 width:27rpx;
height:33rpx;

}
.barLine{
  height: 32rpx;
  display: block;
  width: 100%;
  background-image: url("data:image/png;base64,");

  background-size: 100%;
}
.color3{color:#333;}
.color9{color:#999;}
.color42{color:#424242;}
.qr-view text{
  display: block;
  line-height: 1;
}
.qr-view{
  background: #fff;
  flex-direction: column;
  padding: 30rpx;
  padding-bottom: 64rpx;
  align-items: center;
  text-align: center;
}
.qr-view>view{
  flex-direction: column;
  text-align: center;
  justify-content: center;
}
.qr-view>view:first-child .color3{
  font-size: 72rpx;
  margin-top: 80rpx;
}
.qr-view>view:first-child .color9{
  font-size: 24rpx;
  margin-top: 24rpx;
}
.qr-view>view:nth-child(2) .color3{
  font-size: 26rpx;
  margin-top: 96rpx;
}
.qr-view>view:nth-child(2) .barcode{
 width:560rpx;
height:120rpx;
margin-top: 24rpx;

}
.qr-view>view:nth-child(2) .color42{
  font-size: 28rpx;
  margin-top: 24rpx;
}
.qr-view>.erwei{
  width:260rpx;
height:260rpx;
margin-top: 56rpx;
}
.depict{
  border-top:1rpx dashed rgba(214,214,214,1);
  display: block;
  background: #fff;
  padding: 48rpx 30rpx;
}
.depict text{
  display: block;
  line-height: 1;
}
.depict .color3{
  margin-bottom: 24rpx;
  font-size: 28rpx;
}
.depict .color9{
  font-size: 26rpx;
}
.depict .ul{
  display: block;
  margin-top: 48rpx;
}