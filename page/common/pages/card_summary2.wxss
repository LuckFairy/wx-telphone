page{
  background: #ffa438;
  width: 100%;
  height: auto;
}
.box{
  margin: 35rpx;
  width: 680rpx;
  /* height: 812rpx; */
  border-radius: 8rpx;
  box-shadow:  10rpx 16rpx 0.6 #ff8400;
  margin-top: 80rpx;
  flex-direction: column;
}
.qr-top{
background: #fff;
padding: 40rpx 0 30rpx 0;
font-size:28rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(66,66,66,1);
  align-items: center;
}
.qr-top image{
  margin: 0 10rpx 0 24rpx;
 width:48rpx;
height:48rpx;
border-radius:50%;


}
.text-top{
  background: #fff;
  justify-content: space-between;
  padding-bottom: 8rpx;
}
.text-top text{
  display: block;
}
.text-top>view:first-child{
  flex-direction: column;
  font-size: 24rpx;
  margin-left: 80rpx;
}
.text-top>text:last-child{
font-size: 48rpx;
margin-right: 26rpx;
}
.barLine{
  height: 32rpx;
  display: block;
  width: 100%;
  background-image: url("data:image/png;base64,");

  background-size: 100%;
}
.color3{color:#333;}
.color9{color:#999;}
.color42{color:#424242;}
.qr-view text{
  display: block;
  line-height: 1;
}
.qr-view{
  background: #fff;
  flex-direction: column;
  padding:30rpx;
  padding-bottom: 64rpx;
  align-items: center;
  text-align: center;
}
.qr-view>view{
  flex-direction: column;
  text-align: center;
  justify-content: center;
}
.qr-view>view.qr-view-code .color3{
  font-size: 26rpx;
}
.qr-view>view.qr-view-code .barcode{
width:592rpx;
height:144rpx;
margin-top: 24rpx;

}
.qr-view>view.qr-view-code .color42{
  font-size: 40rpx;
  margin-top: 24rpx;
}
.qr-view>.erwei{
  width:300rpx;
height:300rpx;
/* margin-top: 40rpx; */
}
.depict{
  border-top:1rpx dashed rgba(214,214,214,1);
  display: block;
  background: #fff;
  padding: 48rpx 30rpx;
}
.depict text{
  display: block;
  line-height: 1;
}
.depict .color3{
  margin-bottom: 24rpx;
  font-size: 28rpx;
}
.depict .color9{
  font-size: 26rpx;
}
.depict .ul{
  display: block;
  margin-top: 48rpx;
}