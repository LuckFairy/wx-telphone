
@import'mycard.wxss';

@import'mycardHistory.wxss';

.card-item-btn-view-normal{
  margin-top:10rpx !important;
}

.card-item-up-middle{
  margin-left:0rpx !important;
  margin-right:50rpx !important;
}

.normal-item{
  margin-top:0px !important;
  margin-bottom:0rpx !important;
}

.no-data-btn, .no-data-text {
  font-size: 28rpx;
  text-align: center;
}

.no-data-btn, .no-data-text, text {
  text-align: center;
}

.main-container, .no-data-view {
  -webkit-box-direction: normal;
  -webkit-box-orient: vertical;
}

.main-container, .mon-price, .no-data-view {
  -webkit-box-orient: vertical;
}

.no-data-view {
  -webkit-flex-direction: column;
  flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  padding-top: 100rpx;
}

.no-data-image {
  width: 148rpx;
  height: 162rpx;
  margin-bottom: 60rpx;
}

.no-data-text {
  color: #999;
  line-height: 130%;
}

.no-data-btn {
  width: 176rpx;
  height: 60rpx;
  margin-top: 38rpx;
  background: #ffe71c;
  border-radius: 6rpx;
  line-height: 60rpx;
}

page, scroll-view, swiper {
  height: 100%;
}

.search-current, .search-select {
  line-height: 80rpx;
  height: 80rpx;
}

.go-add {
  font-size: 22rpx;
}

page {
  width: 100%;
  background: #efefef;
}

swiper {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 750rpx;
}

scroll-view {
  display: block;
  background-color: #eee;
}

text {
  display: block;
  overflow: hidden;
  margin: 0;
  padding: 0;
}

.mom-list, .search-msg {
  display: -webkit-box;
  display: -webkit-flex;
}

.main-container {
  -webkit-flex-direction: column;
  flex-direction: column;
  width: 750rpx;
  margin-top: 104rpx;
}

.search-msg {
  display: flex;
  width: 750rpx;
  height: 88rpx;
  border-top: 1rpx solid #efefef;
  background: #fff;
  margin-bottom: 16rpx;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
    z-index: 222222;
}

.search-input {
  width: 530rpx;
  height: 60rpx;
  position: relative;
  background: #efefef;
  margin-left: 21rpx;
  margin-top: 14rpx;
  border-radius: 5rpx;
}

.search-image {
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  left: 20rpx;
  top: 15rpx;
}

.search-text {
  position: absolute;
  font-size: 26rpx;
  color: #1b1b1b;
  left: 60rpx;
  display: inline-block;
  width: 450rpx;
  height: 60rpx;
}

.search-select {
  position: relative;
  display: block;
  margin-left: 34rpx;
  margin-top: 7rpx;
}

.search-current {
  font-size: 0rpx;
}

.search-current-text {
  font-size: 28rpx;
  color: #1b1b1b;
  height: 80rpx;
  display: inline-block;
}

.search-current-trangle {
  display: inline-block;
  width: 0;
  height: 0;
  border-left: 10rpx solid transparent;
  border-right: 10rpx solid transparent;
  border-top: 15rpx solid #000;
  margin-top: 32rpx;
  margin-left: 18rpx;
}

.wait-choose {
  position: fixed;
  top: 88rpx;
  z-index: 999999999;
  background: #fff;
  width: 750rpx;
  font-size: 0rpx;
  display: block;
  height: 108rpx;
  line-height: 108rpx;
  padding-top: 23rpx;
}

.tranClass {
  height: 112rpx;
  transition: height 2s;
  -webkit-transition: width 2s;
}

.online-offline {
  display: inline-block;
  font-size: 28rpx;
  height: 60rpx;
  line-height: 60rpx;
  width: 160rpx;
  border: solid 1rpx #efefef;
  border-radius: 50rpx;
  margin-left: 24rpx;
}

.mom-list, .mom-list-two {
  width: 750rpx;
  height: 200rpx;
  background-size: 710rpx 200rpx;
  background-repeat: no-repeat;
  background-position: 20rpx 0rpx;
  margin-bottom: 20rpx;
}

.mom-list {
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
}

.trues {
  background-image: url(data:image/png;base64,);
}

.mom-list-two {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  background-image: url(data:image/png;base64,);
}

.onlinecard,.mendiancard {
  background-image: url("data:image/png;base64,");

}

.mon-list-left {
  display: block;
}

.mon-image-title {
  display: block;
  font-size: 0rpx;
}

.mon-image, .mon-title {
  display: inline-block;
  margin-top: 20rpx;
}

.mon-image {
  width: 70rpx;
  height: 70rpx;
  margin-left: 36rpx;
}

.mon-day, .mon-price, .mon-title {
  margin-left: 50rpx;
  text-align: left;
}

.mon-title {
  font-size: 32rpx;
  color: #1b1b1b;
  vertical-align: top;
  width: 400rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.mon-day, .mon-price {
  overflow: hidden;
  width: 450rpx;
  text-overflow: ellipsis;
}

.mon-price-day {
  display: block;
  font-size: 0rpx;
  margin-top: 18rpx;
}

.mon-price {
  -webkit-line-clamp: 1;
}

.mon-day, .mon-price {
  font-size: 28rpx;
  color: #a9a9a9;
  display: block;
}

.mon-day {
  margin-top: 25rpx;
  white-space: nowrap;
}

.mon-list-right {
  display: block;
}

.mon-money-right {
  font-size: 0rpx;
  display: block;
  width: 210rpx;
  text-align: center;
  margin-top: 34rpx;
}

.money-right-text {
  display: inline-block;
  font-size: 40rpx;
  color: #fff;
}

.mon-right-go {
  display: block;
  margin-top: 30rpx;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  color: #fff;
  font-size: 28rpx;
  border: solid 1rpx #fff;
  border-radius: 6rpx;
  width: 140rpx;
  margin-left: 29rpx;
}

.shopCard, .xianshangCard {
  background: #ffe71c;
}
