}

.wait-choose {
  position: fixed;
  top: 138rpx;
  z-index: 999999999;
  background: #fff;
  width: 750rpx;
  font-size: 0rpx;
  display: block;
  height: 160rpx;
  line-height: 50rpx;
  padding-top: 23rpx;
}
.footer-choose{
  width: 750rpx;
  height: 98rpx;
  position: fixed;
  background: #fff;
  bottom: 0;
  z-index: 999999999;
}
.footer-choose text{
  display: inline-block;
  margin: 30rpx 0;
  width: 375rpx;
  line-height: 38rpx;
  color: #999;
}
.footer-choose text:first-child{
  border-right: solid #e5e5e5 1rpx;
}
.footer-choose .category{
  color: #ffa438;
}
.tranClass {
  height: 112rpx;
  transition: height 2s;
  -webkit-transition: width 2s;
}

