.wrap, page {
  background: #efefef;
  display: block;
}

.navigation{
  display:flex;
  flex-direction:column;
  height:100%;
  background-color: rgba(239, 239, 239, 1);

}

.child-list, .list-tab {
  display: -webkit-box;
  display: -webkit-flex;
}

.image-all{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 70rpx;
  height: 80rpx;
}
.image-all-content{
  width: 53rpx;
	height: 27rpx;
}

.title{
  width: 750rpx;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  background-color: rgba(255, 255, 255, 1);
}

.child-list {
  /* display: flex; */
   width: 670rpx; 
  height: 80rpx;
  line-height: 80rpx;
  background: #fff;
  margin-bottom: 20rpx;
  white-space:nowrap;
}

.list-tab {
  font-size: 0;
  width: 188rpx;
  height: 78rpx;
  text-align: center;
  /* display: flex; */
  display:inline-block;

  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  color: #999;
  
}

.child-list-text {
  font-size: 30rpx;
}

.current {
  border-bottom: 4rpx solid #ffe71c;
  color: #1b1b1b;
}

.mom-list ,.mom-list-two {
 background-image: url("data:image/png;base64,");
}

.mom-list, .mom-list-two {
  position: relative;
  width: 750rpx;
  height: 220rpx;
  display: flex !important;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  background-size: 710rpx 220rpx;
  background-repeat: no-repeat;
  background-position: 20rpx 0;
  margin-bottom: 20rpx;
}
/* .mom-list-two {
  background-image: url(data:image/png;base64,);
} */
 .mon-list-tip{
   position: absolute;
   left: 40rpx;
   top: 10rpx;
   z-index: 1000;
   font-size: 18rpx;
   color:#fff;
   width: 116rpx;
	height: 30rpx;
	background-color: #ffa438;
	border-radius: 0px 0px 16rpx 0px;
  justify-content: center;
 }
.mon-list-left {
  justify-content: flex-start;
}

.mon-image-title {
  display: block;
  font-size: 0;
}

.content{
  height: 100%;
}

.mon-image {
  width: 140rpx;
	height: 140rpx;
  display: inline-block;
  margin-top: 40rpx;
  margin-left: 36rpx;
  margin-right: 20rpx;
}

.mon-title {
  display: inline-block;
  margin-top: 40rpx;
  font-size: 26rpx;
  color: #454545;
  vertical-align: top;
  width: 310rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.mon-price-day {
  display: block;
  font-size: 0;
  /* margin-top: 28rpx; */
}

 .mon-price {
  font-size: 26rpx;
  color: #343434;
  display: block;
  margin: 22rpx 0 26rpx;
}
.mon-day{
  font-size: 22rpx;
	color: #454545;
}

.mon-list-right {
  display: block;
  background-color: #ffa438;
  margin:20rpx 42rpx 20rpx 0;
}
.mom-list-two .mon-list-right{
  background-color: #c7c7c7;
}
.mon-money-right {
  font-size: 0;
  display: block;
  margin-top: 30rpx;
}

.money-right-cn {
  display: inline-block;
  font-size: 50rpx;
  color: #fff;
  margin-top: 15rpx;
}

.money-right-text {
  font-size: 50rpx;
  width: 175rpx;
}

.mon-right-go, .money-right-text {
  display: block;
  color: #fff;
  text-align: center;
}

.mon-right-go {
  font-size: 28rpx;
  margin-top: 20rpx;
  border: 1rpx solid #efefef;
  border-radius: 6rpx;
  width: 140rpx;
  margin-left: 20rpx;
}

.no-data-view {
  flex-direction: column;
  align-items: center;
  padding-top: 100rpx;
}

.no-data-image {
  width: 148rpx;
  height: 162rpx;
  margin-bottom: 60rpx;
}

.no-data-text {
  text-align: center;
  color: #999;
  line-height: 130%;
  font-size: 28rpx;
}

.no-data-btn {
  width: 176rpx;
  height: 60rpx;
  margin-top: 38rpx;
  text-align: center;
  background: #ffe71c;
  border-radius: 6rpx;
  font-size: 28rpx;
  line-height: 60rpx;
}


.navbar {
  position: fixed;
  z-index: 2;
  background-color: RGB(247, 247, 247);
  width: 100%
}

.navbar-content {
  display: flex;
  flex-direction: column;
  width: 100%
}

.nav-1{  
  flex: none;  
  display: flex;   
  background: #fff;  
  background-color: RGB(247, 247, 247);
  height: 80rpx;
  width: 100%
}

.picker {
background-color: rgba(255, 255, 255, 1);
}


.nav-1 .wrapBar {
  display: flex;
}

.nav-1 .item{  
  position: relative;   
  text-align: center;  
  font-size: 30rpx;
  line-height: 80rpx;  
  color: gray;
  display: inline-block;
  padding-left: 20rpx;
  padding-right: 20rpx;
}  

.nav-1 .item.active{  
  color: black;
}  

.nav-1 .item.active:after{  
  content: "";  
  display: block; 
  position: absolute;  
  bottom: 0;  
  left: 0;  
  right: 0; 
  height: 4rpx;  
  background: #ffe71c;  
}  


button {
  background: transparent;
}

button::after {
  border: 0;
}

.navbarBtn {
  flex: 0 0 15%;
}

.navbarBtn.icon {
  transform: rotate(90deg);
  width: 20rpx;
  height: 40rpx;
}

.navbarBtn.icon.active{
  transform: rotate(-90deg);
}

.blankblock {
  flex: 0 0 10%;
}

/* .nav-1-left {
  white-space: nowrap;
  overflow: hidden;
  flex: 0 0 85%;
} */

.nav-1-left {
  white-space: nowrap;
  overflow: hidden;
  flex: 0 0 85%;
  background-color: transparent;
}


.item.left2Font {
  color: black;
}

.spitLine {
  margin: 0;
  height: 2rpx;
  background-color: gray;
}


.picker-content{
  background: #fff;
  height: 1500rpx;
}

.picker-contain {
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
   width: 100%;
   /* height: 160rpx; */
}

.picker-contain-item {
  width: 120rpx;
	height: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
	background-color: rgba(255, 255, 255, 1);
  color: #999999;
  margin-top: 20rpx;
	border-radius: 20rpx;
  font-size: 26rpx;
  margin-left:25rpx; 
}

.item-current{
  background-color: rgba(255, 231, 28, 1);
  color: #202020;
}



.picker-contain-bg {
  position: fixed;
  background-color: transparent;
  transition: all 1s;
  width: 100%;
  height: 100%;
  z-index: 1;
  top: 0;
  opacity: 0
}

.picker-contain-bg.show {
  opacity: 0.5;
}

.picker-contain .item {
  background-color: RGB(215, 215, 215);
  border-radius:20px;
  margin: 20rpx;
  padding: 50rpx;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  font-size: 30rpx;
  color: RGB(161, 161, 161);
}

.picker-contain .item.active{  
  color: black;
  background-color: RGB(153, 153, 153);
} 

swiper-item scroll-view {
  display: inline;
}

swiper.itemContainer{
  padding-top: 100rpx;
  height: 100%;
}

::-webkit-scrollbar{
width: 0;
height: 0;
color: transparent;
}



