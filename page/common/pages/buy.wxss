/*购买成功提示开始  */

.matte-hint-father {
  display: block;
  width: 750rpx;
  min-height: 100%;
  position: fixed;
  background: #494949;
  z-index: 9999999999;
  top: 0rpx;
}

.matte-hint {
  width: 626rpx;
  height: 500rpx;
  background: #fff;
  position: fixed;
  border-radius: 15rpx;
  z-index: 999999;
  top: 318rpx;
  left: 62rpx;
  font-size: 0rpx;
  display: block;
}

.matte-total {
  display: block;
}

.matte-close {
  width: 30rpx;
  height: 30rpx;
  float: right;
  margin-top: 20rpx;
  margin-right: 20rpx;
}

.matte-close::after {
  clear: both;
  content: '.';
  display: block;
  width: 0;
  height: 0;
  visibility: hidden;
}

.matte-icon {
  height: 200rpx;
  width: 160rpx;
  clear: both;
  margin: 0 auto;
}

.matte-success {
  clear: both;
  font-size: 46rpx;
  color: #1b1b1b;
  text-align: center;
  margin-top: 60rpx;
}

.matte-detail {
  font-size: 30rpx;
  color: #999;
  text-align: center;
  margin-top: 30rpx;
}

/*购买成功提示结束  */

