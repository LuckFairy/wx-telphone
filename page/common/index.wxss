/*优惠券领取  */
.coupon-area{
  display: block;
  color:#fff;
  font-size: 20rpx;
  border-right: none;
  border-left: none;
  padding: 16rpx 24rpx;
  background: inherit;
  line-height: 1;
}
.coupon-area text{
  font-size: 24rpx;
}
.coupon-area .font-big{
  font-size: 70rpx;
}
.coupon-area-up{
  justify-content: space-between;
}
.coupon-up-item{
  flex: 1;
  flex-direction: column;
  padding: 26rpx 0 16rpx;
  background-color: #ff3030;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  margin-right: 10rpx;
  justify-content: space-between;
  align-items: center;
  
}
.coupon-up-item:last-child{
  margin-right: 0;
}
.coupon-up-item view:first-child{
  flex-direction: column;
}
.coupon-up-item view:first-child view{
  flex-direction: row;
  align-items: flex-end;
  margin-bottom: 5rpx;
}
.coupon-up-item view:last-child{
  justify-content: flex-end;
  align-items: center;
}
.coupon-up-item view:last-child image{
  width: 24rpx;
  height: 24rpx;
  margin-left: 16rpx;
}
.coupon-area-down{
   margin-top: 7rpx;
   background-color: #ff3030;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  justify-content: space-between;
  background-image: url("data:image/png;base64,");
}
.coupon-down-left{
  align-items: center;
}
.coupon-down-left view:first-child{
  align-items: flex-end;
  padding: 39rpx 12rpx 39rpx 30rpx;
}
.coupon-down-left view:last-child{
  justify-content: flex-start;
  flex-direction: column;
  align-items: center;
}
.coupon-down-right{
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-right: 50rpx;
}
.coupon-down-right text:first-child{
  margin-bottom: 8rpx;
}
