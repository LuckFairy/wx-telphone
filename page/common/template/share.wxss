.share-content{
  width: 640rpx;
  background: -moz-linear-gradient(225deg, #ff8828, #ff5358 );
  background: -webkit-linear-gradient(225deg,#ff8828,#ff5358 );
  background: -o-linear-gradient(225deg, #ff8828, #ff5358);
}
.shareCancel{
  width: 36rpx;
	height: 36rpx;
	position:absolute;
  right:20rpx;
  top:20rpx;
  background-repeat: no-repeat;
  background-size: 100%;
  background-position: center;
  background-image: url("data:image/png;base64,");
}
.share-title{
  display: block;
  margin:50rpx 0 66rpx;
  color:#fff;
  
}
.share-firstText{
  display: block;
  font-size: 40rpx;
   line-height: 1;
   text-align: center;
}
.share-twoText {
  display: block;
	font-size: 26rpx;
	 line-height: 1;
   margin-top: 18rpx;
   text-align: center;
}
.share-btn{
  width: 560rpx !important;
	height: 80rpx;
  line-height: 80rpx;
	background-color: #ffe71c;
	border-radius: 10rpx;
  font-size: 32rpx;
	color: #9b5c29 !important;
  overflow:inherit;
  margin-right:auto;
  margin-bottom:60rpx;
}
.share-line{
  width: 560rpx;
  height:28rpx;
  margin: 4rpx auto 16rpx;
  text-align: center;
  font-size: 28rpx;
	line-height: 32rpx;
	color: #ffe71c;
}
.share-line-img{
  width:100%;height:100%;
  background-repeat: no-repeat;
  background-size: 100%;
  background-position: center;
  background-image: url("data:image/png;base64,");
}
.share-bottom-button-wrap{
  display: block;
  margin: 20rpx 0 40rpx;
}
.shareConfirm{
  width: 320rpx;
	height: 70rpx;
  line-height: 70rpx;
	background-color: #ffe71c;
	border-radius: 10rpx;
  color: #9b5c29;
  font-size: 32rpx;
  overflow:inherit;
  margin-right:auto;
}


